	<queryString>
		<![CDATA[SELECT
	g.creation_date,
	g.name,
	(SELECT c.name FROM gameclub.console c WHERE c.id=gc.console) AS console
FROM gameclub.game g
	LEFT JOIN gameclub.game_console gc ON gc.game=g.id
WHERE
	($P{console} IS NULL OR  $P{console}=CAST(gc.console AS TEXT))
ORDER BY g.creation_date]]>
	</queryString>
