	<queryString>
		<![CDATA[SELECT 
	pu.name,
	pu.last_name,
	pu.document,
    t.creation_date,
    t.transaction,
    t.game,
    t.console,
    t.weeks,
	bytea_to_decimal(t.balance_part,pu.private_key) AS balance_part,
	bytea_to_decimal(t.debit_balance,pu.private_key) AS debit_balance,
	bytea_to_decimal(t.debit_card,pu.private_key) AS debit_card
FROM public_user pu
	INNER JOIN transaction t ON 
	 t.public_user_transaction = pu.id
WHERE pu.id= $P{ide}]]>
	</queryString>
